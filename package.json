        "express": "^4.17.1",
        "graphql-iso-date": "^3.6.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "mongoose": "^5.9.15"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.32",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.6",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.152",
        "@types/mongoose": "^5.7.21",
        "@types/node": "^14.0.5",
